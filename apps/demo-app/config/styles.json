{
  "red": "#e74c3c",
  "red-dark": "#c0392b",
  "orange": "#e67e22",
  "orange-dark": "#d35400",
  "yellow": "#f1c40f",
  "amber": "#f39c12",
  "teal": "#1abc9c",
  "teal-dark": "#16a085",
  "green": "#2ecc71",
  "green-dark": "#27ae60",
  "light-blue": "#3dbdff",
  "blue": "#3498db",
  "blue-dark": "#2980b9",
  "purple": "#9b59b6",
  "purple-dark": "#8e44ad",
  "navy-blue": "#34495e",
  "navy-blue-dark": "#2c3e50",
  "silver": "#bdc3c7",
  "white": "#fff",
  "black": "#090a0a",
  "gray": "$silver",
  "gray-light": "#e6e9ea",
  "gray-dark": "#869198",
  "gray-darker": "#545e64",
  "primary-color": "$navy-blue",
  "secondary-color": "$blue",
  "dark-color": "$navy-blue-dark",
  "alert-color": "$red",
  "success-color": "$green",
  "warning-color": "$yellow",
  "borders-dark": "rgba($black, .15)",
  "borders-light": "rgba($white, .15)",
  "btn-link-color": "$secondary-color",
  "divider-color-dark": "$borders-dark",
  "divider-color-light": "$borders-light",
  "global-bg": "$gray-light",
  "global-content-bg": "isitlight($global-bg, $white, lighten($global-bg, 10))",
  "global-headers-bg": "isitlight($global-bg, darken($global-bg, 10), lighten($global-bg, 10))",
  "global-shadow-color": "rgba($black, .18)",
  "global-overlay-bg": "isitlight($global-bg, rgba($black, .5), rgba($black, .7))",
  "global-border-dark": "rgba($black, .15)",
  "global-border-light": "rgba($white, .15)",
  "global-border-width": "rem-calc(1)",
  "global-link-color": "$blue",
  "global-rem": "16px",
  "global-font-size": "100%",
  "global-font-family": "'Roboto', 'Helvetica Neue', 'Helvetica', Helvetica, Arial, sans-serif",
  "global-padding": "1rem",
  "global-margin": "1rem",
  "global-slide-speed": ".2s",
  "grid-padding": "$global-padding",
  "font-size-xlarge": "2rem",
  "font-size-large": "1.5rem",
  "font-size-normal": "$global-font-size",
  "font-size-small": ".875rem",
  "font-size-tiny": ".75rem",
  "font-size-icon": "1.5rem",
  "font-size-code": "1em",
  "font-icon-line-height": "2rem",
  "font-primary-dark": "rgba($black, .9)",
  "font-primary-light": "$white",
  "font-secondary-dark": "rgba($black, .6)",
  "font-secondary-light": "rgba($white, .75)",
  "font-disabled-dark": "rgba($black, .30)",
  "font-disabled-light": "rgba($white, .35)",
  "font-hint-dark": "$font-disabled-dark",
  "font-hint-light": "$font-disabled-light",
  "font-icon-dark": "$font-secondary-dark",
  "font-icon-light": "$font-secondary-light",
  "btn-solid-bg-darken": "15",
  "btn-solid-bg-lighten": "35",
  "btn-radius": "rem-calc(2)",
  "btn-font-size": "$font-size-small",
  "btn-font-weight": "600",
  "btn-focus-bg-adjust": "10",
  "btn-hover-bg-adjust": "$btn-focus-bg-adjust",
  "btn-submit-color": "$global-link-color",
  "btn-submit-adjust": "9",
  "btn-padding": "0 .75rem",
  "btn-margin": ".5rem",
  "form-bg": "transparent",
  "form-field-bg": "$global-content-bg",
  "form-input-color": "isitlight($form-field-bg, $font-primary-dark, $font-primary-light)",
  "form-label-color": "isitlight($form-field-bg, $font-secondary-dark, $font-secondary-light)",
  "form-heading-bg": "$global-headers-bg",
  "form-border-color": "isitlight($form-field-bg, $global-border-dark, $global-border-light)",
  "form-border-width": "$global-border-width",
  "form-focused-color": "$global-link-color",
  "form-field-height": "rem-calc(56)",
  "form-field-label-size": ".75rem",
  "body-bg": "$gray-light",
  "body-font-family": "$global-font-family",
  "body-font-color": "isitlight($body-bg, $font-primary-dark, $font-primary-light)",
  "body-antialiased": "true",
  "textfield-bg": "$form-field-bg",
  "textfield-color": "$form-input-color",
  "textfield-label-color": "$form-label-color",
  "textfield-label-size": "$form-field-label-size",
  "textfield-focused-color": "$global-link-color",
  "textfield-transition-speed": ".25s",
  "textfield-error-color": "$alert-color",
  "textfield-icon-color": "isitlight($textfield-bg, $font-hint-dark, $font-hint-light)",
  "appbar-bg": "$primary-color",
  "appbar-color": "$body-font-color",
  "appbar-height": "rem-calc(56)",
  "appbar-height-small": "$appbar-height * .857",
  "appbar-search-bg": "isitlight($appbar-bg, $global-border-dark, $global-border-light)",
  "appbar-spacing": ".75rem",
  "appbar-dropdown-bg": "$global-content-bg",
  "card-bg": "$global-content-bg",
  "card-shadow-color": "$global-shadow-color",
  "card-edge-color": "isitlight($card-bg, rgba($black,.2), rgba($black, .35))",
  "card-padding": "$global-padding/2",
  "checkbox-bg": "$textfield-bg",
  "checkbox-focused-color": "$global-link-color",
  "checkbox-unchecked-color": "isitlight($checkbox-bg, $font-hint-dark, $font-hint-light)",
  "checkbox-checked-color": "isitlight($checkbox-bg, $font-primary-dark, $font-primary-light)",
  "checkbox-error-color": "$textfield-error-color",
  "unclassified-bg": "$green-dark",
  "unclassified-color": "$font-secondary-light",
  "confidential-bg": "$light-blue",
  "confidential-color": "$font-secondary-dark",
  "secret-bg": "$red",
  "secret-color": "$font-secondary-dark",
  "top-secret-bg": "$orange",
  "top-secret-color": "$font-secondary-dark",
  "class-bar-height": "rem-calc(24)",
  "class-bar-height-sm": "rem-calc(6)",
  "class-bar-hide-text-sm": "true",
  "counter-bg": "$textfield-bg",
  "counter-color": "isitlight($counter-bg, $font-primary-dark, $font-primary-light)",
  "counter-label-color": "isitlight($counter-bg, $font-hint-dark, $font-hint-light)",
  "counter-focused-color": "$global-link-color",
  "datepicker-bg": "$form-field-bg",
  "datepicker-field-border": "1px solid isitlight($datepicker-bg, $divider-color-dark, $divider-color-light)",
  "datepicker-header-bg": "$global-headers-bg",
  "datepicker-focused-color": "$secondary-color",
  "datepicker-active-bg": "$secondary-color",
  "datepicker-now-bg": "isitlight($datepicker-bg, $gray-dark, $gray-light)",
  "datepicker-overlay-bg": "rgba($black, .5)",
  "datepicker-hover": "$datepicker-header-bg",
  "dropdown-bg": "$textfield-bg",
  "dropdown-color": "$textfield-color",
  "dropdown-shadow": "0 0 .25rem 0 $global-shadow-color",
  "dropdown-active-color": "$global-link-color",
  "dropdown-label-color": "$textfield-label-color",
  "dropdown-error-color": "$textfield-error-color",
  "dropdown-options-bg": "$dropdown-bg",
  "dropdown-options-border": "$global-border-width solid isitlight($dropdown-options-bg, $global-border-dark, $global-border-light)",
  "dropdown-focused-color": "$global-link-color",
  "fileloader-bg": "$textfield-bg",
  "fileloader-label-color": "$textfield-label-color",
  "fileloader-focused-color": "$global-link-color",
  "fileloader-error-color": "$textfield-error-color",
  "list-tile-bg": "$global-content-bg",
  "list-heading-color": "isitlight($list-tile-bg, $font-primary-dark, $font-primary-light)",
  "list-row-divider": "1px solid isitlight($list-tile-bg, $global-border-dark, $global-border-light)",
  "list-content-padding": ".5rem",
  "list-active-color": "$global-link-color",
  "list-action-secondary-bg": "isitlight($list-tile-bg, darken($list-tile-bg, 20), lighten($list-tile-bg, 20))",
  "login-bg": "$global-bg",
  "login-form-bg": "$global-content-bg",
  "menu-bg": "$global-content-bg",
  "menu-color": "isitlight($menu-bg, $font-primary-dark, $font-primary-light)",
  "menu-item-active": "$global-link-color",
  "menu-min-width": "rem-calc(200)",
  "menu-sub-bg": "isitlight($menu-bg, rgba($black, .05), rgba($white, .05))",
  "menu-panel-slide-speed": "$global-slide-speed",
   "breakpoints": {
    "sm": "0",
    "md": "600",
    "lg": "900",
    "xl": "1200",
    "xxl": "1800"
  },
  "bp-ranges": "up, down, only",
  "modal-overlay-bg": "$global-overlay-bg",
  "modal-header-bg": "$global-headers-bg",
  "modal-header-padding": "$global-padding / 2",
  "modal-divider-color": "$modal-header-bg",
  "modal-full-bg": "$modal-overlay-bg",
  "modal-width-xs": "rem-calc(300)",
  "modal-width-s": "rem-calc(450)",
  "modal-width-m": "rem-calc(600)",
  "modal-width-l": "rem-calc(800)",
  "notification-bg": "isitlight($body-bg, rgba($black, .9), rgba($white, .9))",
  "notification-color": "isitlight($notification-bg, $font-primary-dark, $font-primary-light)",
  "notification-btn-color": "isitlight($notification-bg, $font-secondary-dark, $font-secondary-light)",
  "panel-animation-speed": "250ms",
  "panel-bg": "$global-content-bg",
  "panel-header-bg": "$global-headers-bg",
  "panel-header-height": "$appbar-height",
  "panel-header-height-sm": "$appbar-height-small",
  "panel-divider-color": "$panel-header-bg",
  "panel-height": "rem-calc(300)",
  "panel-overlay-bg": "$global-overlay-bg",
  "panel-shadow-color": "$global-shadow-color",
  "panel-width": "rem-calc(300)",
  "table-bg": "transparent",
  "table-th-bg": "transparent",
  "table-td-bg": "$global-content-bg",
  "table-td-bg-active": "$global-link-color",
  "table-border-color": "isitlight($table-td-bg, transparentize($global-border-dark, .07), transparentize($global-border-light, .07))",
  "table-border": "$global-border-width solid $table-border-color",
  "table-accordion-bg": "$table-td-bg",
  "table-simple-padding": ".25rem",
  "tabs-bg": "isitlight($body-bg, rgba($black, .12), rgba($white, .12))",
  "tabs-color": "isitlight($body-bg, $font-secondary-dark, $font-secondary-light)",
  "tabs-caption-color": "isitlight($body-bg, $font-primary-dark, $font-primary-light)",
  "tabs-caption-size": "rem-calc(10)",
  "tabs-hover-color": "$btn-link-color",
  "tabs-active-bg": "$body-bg",
  "tabs-active-color": "isitlight($body-bg, $font-primary-dark, $font-primary-light)",
  "toggle-switch-bg": "isitlight($global-content-bg, $global-border-dark, $global-border-light)",
  "toggle-switch-on-bg": "$secondary-color",
  "toggle-switch-knob-color": "isitlight($toggle-switch-bg, $black, $white)",
  "toggle-switch-height": "1.375rem",
  "toggle-switch-focused-color": "rgba($toggle-switch-knob-color, .7)",
  "full-screen-bg": "isitlight($global-bg, $gray-darker, $gray-light)"
}
